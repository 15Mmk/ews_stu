{
  "XGBClassifier": {
    "model": "xgboost.XGBClassifier",
    "params": {
      "random_state": 42,
      "n_estimators": 400,
      "learning_rate": 0.05,
      "max_depth": 6,
      "scale_pos_weight": 1,
      "eval_metric": "logloss",
      "use_label_encoder": false,
      "n_jobs": -1
    }
  },
  "LGBMClassifier": {
    "model": "lightgbm.LGBMClassifier",
    "params": {
      "random_state": 42,
      "n_estimators": 400,
      "learning_rate": 0.05,
      "max_depth": 6,
      "scale_pos_weight": 1,
      "n_jobs": -1
    }
  },
  "CatBoostClassifier": {
    "model": "catboost.CatBoostClassifier",
    "params": {
      "random_seed": 42,
      "iterations": 400,
      "learning_rate": 0.05,
      "depth": 6,
      "scale_pos_weight": 1,
      "verbose": false,
      "task_type": "CPU"
    }
  },
  "RandomForestClassifier": {
    "model": "sklearn.ensemble.RandomForestClassifier",
    "params": {
      "random_state": 42,
      "n_estimators": 400,
      "max_depth": 6,
      "class_weight": "balanced",
      "n_jobs": -1
    }
  },
  "LogisticRegression": {
    "model": "sklearn.linear_model.LogisticRegression",
    "params": {
      "random_state": 42,
      "max_iter": 1000,
      "class_weight": "balanced",
      "solver": "liblinear",
      "n_jobs": -1
    }
  }
